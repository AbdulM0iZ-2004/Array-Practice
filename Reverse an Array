#include <iostream>
using namespace std;

int main(){
    const int size = 5;
    int numbers[size];

    for (int i=0; i < size ; i++){
        cout << "Enter Number " << i+1 << ": ";
        cin >> numbers[i];
    }

    for (int i= size - 1; i >= 0 ; i--){
        cout << numbers[i] << " " ;

    }

}
